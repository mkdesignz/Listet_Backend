{
  "name": "listet_backend",
  "version": "1.0.0",
  "description": "Backend api for Listet",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently -k -n 'TypeScript,Node' -c 'yellow.bold,cyan.bold' 'npm run watch-ts' 'nodemon ./dist/server.js' -i true --default-input-target 1",
    "dev:verbose": "nodemon --watch src/ --exec 'tsc && npm start' --verbose -e ts",
    "watch-ts": "tsc -w",
    "build": "tsc",
    "clean": "rm -rf dist",
    "test": "echo 'Error: no test specified' && exit 1",
    "snyk:protect": "snyk protect",
    "snyk:test": "snyk test",
    "prepare": "npm run snyk:protect",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manuelosorio/Listet_Backend.git"
  },
  "keywords": [
    "Todo",
    "list",
    "angular",
    "express",
    "backend",
    "fullstack"
  ],
  "author": "Manuel Osorio",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/manuelosorio/Listet_Backend.git/issues"
  },
  "homepage": "https://github.com/manuelosorio/Listet_Backend.git#readme",
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "chalk": "^4.0.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "email-templates": "^8.0.3",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-mysql-session": "^2.1.5",
    "express-session": "^1.17.1",
    "helmet": "^4.4.1",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.4.17",
    "pug": "^3.0.1",
    "snyk": "^1.440.1",
    "socket.io": "^3.1.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/chalk": "^2.2.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/email-templates": "^7.1.0",
    "@types/express": "^4.17.6",
    "@types/express-flash": "0.0.2",
    "@types/express-mysql-session": "^2.1.1",
    "@types/express-session": "^1.17.0",
    "@types/mysql": "^2.15.10",
    "@types/node": "^14.14.35",
    "@types/nodemailer": "^6.4.0",
    "@types/socket.io": "^2.1.12",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "prettier": "^2.2.1",
    "nodemon": "^2.0.3",
    "tslint": "^6.1.2",
    "typescript": "^4.1.3"
  },
  "snyk": true,
  "private": true
}
